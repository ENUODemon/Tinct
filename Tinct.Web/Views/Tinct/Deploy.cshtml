
@{
    ViewBag.Title = "Deploy";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Deploy</h2>

<form action="/Tinct/DeployFile" method="post" enctype="multipart/form-data">
    <div class="form-group">
        <label for="exampleInputFile">Task input</label>
        <input type="file" id="exampleInputFile" name="file">
        <p class="help-block">Please select your task file.</p>
    </div>
    <button type="submit" class="btn btn-default">Submit</button>
</form>

